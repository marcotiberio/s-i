<div class="flyntComponent" is="flynt-navigation-footer-columns">
  <div class="container">
    <div class="columns">
      <div class="columns-info">
        <div class="logo">
          <img class="logo-image" src="{{ logo.src }}" alt="{{ logo.alt }}">
        </div>
      </div>
      <div class="columns-menu">
        {% include "Partials/_menu.twig" with { items: menu.items } %}
        <div class="social">
          {% if social %}
            <ul class="socialList">
              {% for socials in social %}
                <li class="socialList-item">
                  <a class="socialList-link" href="{{ socials.url }}" aria-label="{{ socials.platform.label }}">
                    {{ socials.platform.label }}
                  </a>
                </li>
              {% endfor %}
            </ul>
          {% endif %}
        </div>
        {% if logotype %}
        <div class="logotype">
          <figure class="figure">
            <img class="figure-image lazyload" src="{{ logotype.src|resizeDynamic(0, 540) }}">
          </figure>
        </div>
        {% endif %}
        <div class="credits">
          {{ socialContentHtml|e('wp_kses_post') }}
        </div>
      </div>
    </div>
    {% include 'Partials/_backtotop.twig' %}
  </div>
</div>
